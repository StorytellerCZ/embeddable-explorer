{
  "name": "@apollo/explorer",
  "version": "0.1.0",
  "author": "packages@apollographql.com",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6.9"
  },
  "scripts": {
    "start": "tsdx watch",
    "build:umd": "tsdx build --entry src/index-umd.ts --format umd --name embeddable-explorer",
    "build:cjs-esm": "tsdx build --entry src/index.ts --format cjs,esm --name embeddable-explorer",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "typescript:check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "module": "dist/embeddable-explorer.esm.js",
  "size-limit": [
    {
      "path": "dist/embeddable-explorer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/embeddable-explorer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "graphql": "^15.6.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  }
}
